      <th scope="row">{{codigo}}</th>
      <td>{{titulo}}</td>
      <td>{{_id}}</td>
      <td><button class="btn btn-success mt-2" type="submit">
          <i class="far fa-check-circle"> Lan√ßar Nota</i></td>
    </tr>
    {{else}}
